{
  "name": "@qbbsh/starter-cli",
  "type": "module",
  "version": "1.3.2",
  "description": "cli stater",
  "author": "Q.Ben Zheng <zhengqbbb@gmail.com> (https://github.com/Zhengqbbb/)",
  "license": "MIT",
  "homepage": "https://github.com/Zhengqbbb/starter-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhengqbbb/starter-cli"
  },
  "bugs": "https://github.com/Zhengqbbb/starter-cli/issues",
  "keywords": [
    "cli"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "starter-cli": "dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub && vitest",
    "build": "unbuild",
    "start": "starter-cli",
    "cz": "czg",
    "lint": "eslint .",
    "test": "vitest run",
    "postinstall": "simple-git-hooks",
    "prepack": "npm run build",
    "release": "pnpm run-s release:bump release:publish",
    "release:bump": "pnpm bumpp -y -c 'build: :bookmark: publish v%s'",
    "release:publish": "pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@qbbsh/starter-cli": "workspace:*",
    "@types/node": "^18.13.0",
    "bumpp": "^8.2.1",
    "cz-git": "^1.4.1",
    "czg": "^1.4.1",
    "eslint": "^8.34.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "pathe": "^1.1.0",
    "pnpm": "^7.27.0",
    "rimraf": "^4.1.2",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vitest": "^0.28.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm lint"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
